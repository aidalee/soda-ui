{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2015",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve", // 不设置tsx文件会报红色提醒 无法使用 JSX，除非提供了 "--jsx" 标志
    // 下面这几项不设置目前无影响
    // "isolatedModules": true,
    // "downlevelIteration": true,
    // "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"]
    // "allowSyntheticDefaultImports": true
  }
}
